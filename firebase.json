{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
        "source": "**/*.@(eot|otf|ttf|ttc|woff|woff2)",
        "headers": [{
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }]
      },
      {
        "source": "**/*.@(webp|jpg|jpeg|gif|png|svg|css|js|eot|otf|ttf|ttc|woff|woff2)",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=604800"
        }]
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
